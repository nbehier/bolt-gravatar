{
    "name": "leskis/bolt-gravatar",
    "description": "Gravatar Twig Helper",
    "type": "bolt-extension",
    "keywords": [
        "Twig", "Gravatar"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Béhier-Dévigne",
            "homepage": "https://leskis.net",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Leskis\\BoltGravatar\\": "src"
        },
        "classmap": ["lib/ry167/twig-gravatar/TwigGravatar.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Leskis\\BoltGravatar\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Leskis\\BoltGravatar\\BoltGravatarExtension",
        "bolt-icon": "assets/icon.png"
    }
}
